<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="spring.boot.data.mybatis.mapper.UserMapper" >

    <!--<resultMap id="BaseResultMap" type="spring.boot.data.mybatis.po.UserPO" >-->
        <!--<id column="id" property="id" jdbcType="BIGINT" />-->
        <!--<result column="name" property="name" jdbcType="VARCHAR" />-->
        <!--<result column="birthday" property="birthday" jdbcType="TIMESTAMP" />-->
    <!--</resultMap>-->

    <select id="selectByMap" parameterType="map" resultType="spring.boot.data.mybatis.po.UserPO">
      select * from t_user limit #{start},#{page}
    </select>

    <select id="selectByObject" parameterType="spring.boot.data.mybatis.po.UserPO"
        resultType="spring.boot.data.mybatis.po.UserPO">
      select * from t_user where name=#{name} and id=#{id}
    </select>

    <select id="selectById" parameterType="integer" resultType="spring.boot.data.mybatis.po.UserPO">
      select * from t_user where id=#{id}
    </select>

     <select id="selectLike" parameterType="String" resultType="spring.boot.data.mybatis.po.UserPO">
      select * from t_user where name like "%"#{name}"%"
    </select>

</mapper>